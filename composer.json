{
    "name": "drtheuns/apitizer_php",
    "description": "A Laravel querybuilder library for JSON APIs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Randall Theuns",
            "email": "randall@theuns.org"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/http": "^6.0",
        "illuminate/database": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Apitizer\\": "src/Apitizer"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Apitizer\\ServiceProvider"
            ]
        }
    }
}
